Library:
    !table Member:
        MemberId <: int [~pk, ~autoinc]
        name <: string
        city <: string(100)
        amount <: decimal(14.2)
    !table Book:
        BookId <: int [~pk, ~autoinc]
        name <: string
        Author <: string
    !table Borrow:
        MemberId <: Member.MemberId [~pk]
        BookId <: int [~pk]
        DueDate <: date
